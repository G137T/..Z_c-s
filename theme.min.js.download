$(window).load(function(){
    
    var $insta = $("#instagram");
    
    // Check if markup is set to avoid duplicate
    if ( !App.is_Home ) return;
        
    $.ajax({
        url: 'https://www.instagram.com/sigainove/?__a=1',
        dataType: 'json',
        type: 'GET',
        success: function(response) {
            console.log(response);
            try { 
                var data = response.graphql.user.edge_owner_to_timeline_media.edges;
                //data = data.slice(0, 2);
                console.log(data);
                var $instaImages = "";
                for (var k = 0; k < data.length; k++) {
                   var item = data[k].node
                   var image = item.display_url;
                   var likes = item.edge_liked_by.count;
                   var comments = item.edge_media_to_comment.count;
                    $instaImages+= '<li>'
                                +'<a href="https://instagram.com/p/'+item.shortcode+'" target="_blank" rel="noopener nofollow">'
                                    +(!App.tpl_mobile ? '<div class="stats">'
                                        +'<span class="likes"><svg class="i-heart-o i-fw svg-icon"><use xlink:href="#i-heart-o" /></svg> ' + likes + '</span>'
                                        +'<span class="comments"><svg class="i-comments i-fw svg-icon"><use xlink:href="#i-comments" /></svg> ' + comments + '</span>'
                                    +'</div>' : '')
                                    +'<span class="photo" style="background-image:url(' + image + ')"></span>'
                                +'</a>'
                            +'</li>';
                            
                };
                
                 $insta.find('.insta-feed').append($instaImages);
                
            } catch(err) {
                console.log('instagram error: '+err);
                $insta.hide();
                return false;
            }
            
            
            // UX - remove loading
            $insta.find('.full-loading').removeClass('full-loading');
            
            // Init insta carousel on mobile or multiples photos  
            var insta = App.Settings.home.insta.carousel;
            insta.slidesToScroll = 1;
            insta.slidesToShow = 2;
            insta.dots = false;
            insta.centerMode = true;
            insta.nextArrow = insta.responsive[0].settings.nextArrow;
            insta.prevArrow = insta.responsive[0].settings.prevArrow;
            $("#instagram ul").slick(insta);

            
        },
        error: function (data) {
            $insta.hide();
            console.log('Insta: '+data);
        }
    });

});